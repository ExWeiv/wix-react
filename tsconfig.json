{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "jsx": "react",
        "outDir": "../src/public/react",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "removeComments": true,
        "strict": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "esModuleInterop": true,
        "noEmitOnError": false,
        "moduleResolution": "Node",
        "baseUrl": "."
    }
}